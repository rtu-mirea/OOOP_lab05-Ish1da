        super("Login");
        tax.setSize(300, 100);
        tax.setPreferredSize(tax.getSize());
        this.setBounds(500, 500, 900, 230);
        this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);


        Container con1 = this.getContentPane();
        con1.setLayout(new GridBagLayout());
        con1.setEnabled(false);
        Container con = new JPanel();
        con.setLayout(new GridLayout(6, 2, 2, 2));
        con.add(lab1);
        con.add(tex1);
        con.add(lab2);
        con.add(tex2);
        con.add(lab3);
        con.add(tex3);
        con.add(lab4);
        con.add(tex4);
        but1.addActionListener(new ButtonEventListener());
        but2.addActionListener(new ButtonEventListener());
        but3.addActionListener(new ButtonEventListener());
        but4.addActionListener(new ButtonEventListener());
        con.add(but1);
        con.add(but2);
        con.add(but3);
        con.add(but4);
        GridBagConstraints cons = new GridBagConstraints();
        cons.fill = GridBagConstraints.NONE;
        cons.weightx = 0.5;
        cons.gridy   = 0  ;
        Container co = new JPanel();
        co.setLayout(new GridBagLayout());
        co.add(comboBox, cons);
        cons.fill = GridBagConstraints.BOTH;
        cons.weighty = 0;
        cons.gridy   = 1  ;
        cons.fill = GridBagConstraints.NONE;
        cons.weightx = 1;
        cons.gridy   = 1  ;
        co.add(tax, cons);
        cons.fill = GridBagConstraints.BOTH;
        cons.weightx = 2;
        cons.gridy   = 2  ;
        co.add(scroll, cons);
        cons.gridy   = 0  ;
        cons.fill = GridBagConstraints.HORIZONTAL;
        cons.weightx = 0.5;
        cons.gridx   = 0  ;
        con1.add(con, cons);
        cons.fill = GridBagConstraints.HORIZONTAL;
        cons.weightx = 1;
        cons.gridx   = 1  ;
        con1.add(co, cons);